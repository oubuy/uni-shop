<block wx:if="{{show}}"><view><block wx:if="{{addressList&&addressList.length>0}}"><view class="addresslist"><uni-swipe-action vue-id="1" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><uni-swipe-action-item vue-id="{{('2-'+index)+','+('1')}}" options="{{options}}" data-event-opts="{{[['^click',[['onClick',['$event',index,'$0'],[[['addressList','',index,'id']]]]]],['^change',[['change']]]]}}" bind:click="__e" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><view data-event-opts="{{[['tap',[['addressCheck',['$0'],[[['addressList','',index,'id']]]]]]]}}" class="addressItem" bindtap="__e"><view class="addressMain"><text class="username">{{item.username}}</text><text class="telphone">{{item.telphone}}</text><block wx:if="{{item.default==1}}"><text class="default">默认</text></block></view><view class="addressInfo">{{''+item.city+item.address+''}}</view></view></uni-swipe-action-item></block></uni-swipe-action></view></block><block wx:else><view class="empty"><image src="../../static/default/address.png" mode></image><text>用户没有收货地址,请添加</text></view></block><view style="height:90rpx;"></view><view data-event-opts="{{[['tap',[['addUrl',['$event']]]]]}}" class="addBtn" bindtap="__e">添加新地址</view></view></block>